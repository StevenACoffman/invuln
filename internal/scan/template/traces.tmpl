{{- /*
This template is a has overrides used when printing in verbose mode.
*/ -}}

{{- define "Traces" -}}
  {{- if len .}}
    {{- "    "}}Call stacks in your code:{{- "\n" -}}
    {{- range $index, $entry := .}}
      {{- "      "}}#{{inc $index}}: for function {{$entry.Symbol}}{{- "\n" -}}
      {{- range $entry.Trace}}
        {{- "        "}}{{- if .Position}}{{.Position}}: {{end}}{{.Name}}{{- "\n" -}}
      {{- end -}}
    {{- end -}}
  {{- end -}}
{{- end -}}
